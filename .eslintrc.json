{
	"extends": [
		"prettier",
		"airbnb-base",
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"env": {
		"es2022": true,
		"node": true
	},
	"rules": {
		"no-unused-vars": "warn",
		"semi": "off",
		"no-console": 0,
		"comma-dangle": [
			"off"
		],
		"no-tabs": "off",
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": 0,
		"object-curly-newline": "off",
		"import/extensions": "off",
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"printWidth": 120,
				"useTabs": true,
				"tabWidth": 2,
				"semi": false,
				"proseWrap": "always"
			}
		]
	},
	"plugins": [
		"prettier"
	]
}